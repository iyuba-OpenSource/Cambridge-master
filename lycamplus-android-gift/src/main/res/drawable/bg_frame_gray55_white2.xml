<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">
    <!-- 填充颜色 -->
    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="1dp"
        android:color="#55000000"/>
    <solid android:color="#FDFEFC"/>
    <padding
        android:bottom="4dp"
        android:left="20dp"
        android:right="20dp"
        android:top="4dp"/>
    <!--矩形的圆角半径 -->
    <corners android:radius="8dp"/>
</shape>